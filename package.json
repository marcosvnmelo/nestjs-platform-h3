{
  "name": "@marcosvnmelo/nestjs-platform-h3",
  "description": "A H3 adapter for NestJS framework",
  "version": "11.1.14",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "typescript",
    "library",
    "nestjs",
    "h3",
    "platform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "test": "rstest",
    "test:watch": "rstest --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "release": "quick-publish"
  },
  "dependencies": {
    "@fastify/busboy": "3.2.0",
    "h3": "2.0.1-rc.14",
    "path-to-regexp": "8.3.0",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@nestjs/common": "11.1.14",
    "@nestjs/core": "11.1.14",
    "@nestjs/testing": "^11.1.14",
    "@rslib/core": "0.19.6",
    "@rstest/adapter-rslib": "^0.2.0",
    "@rstest/core": "^0.8.5",
    "@types/node": "^24.10.13",
    "@types/supertest": "7.2.0",
    "eslint": "10.0.2",
    "prettier": "3.8.1",
    "quick-publish": "0.7.1",
    "supertest": "^7.2.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0"
  }
}
